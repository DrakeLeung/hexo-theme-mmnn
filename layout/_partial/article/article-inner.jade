//article-inner
//- content of article

section.article-inner
  header.article-header
    h1.article-title
      a(
        href=url_for(post.link? post.link: post.path)
        title=post.title
      )= post.title

    != toc(post.content, {list_number: false})
    
  .article-entry !{post.content}
